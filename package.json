    "remark-html": "^15.0.2",
    "rimraf": "^2.7.1",
    "rollup": "2.77.1",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-typescript2": "^0.32.1",
