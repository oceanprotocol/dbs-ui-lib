    "webpack": "^5.82.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "scss:watch": "cpx \"src/scss/**\" dist/scss -v -w",
    "rollup:watch": "rollup -c -w",
    "build:watch": "npm run clean && npm-run-all -p scss:watch rollup:watch",
    "clean": "rimraf dist",
    "test": "jest",
    "testWatch": "jest --watch",
    "scss:build": "cpx \"src/scss/**\" dist/scss -v",
    "src:build": "cpx \"src/**\" dist/src -v",
    "rollup:build": "rollup -c",
    "build": "npm run clean && npm-run-all -s clean scss:build rollup:build",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "jest": {
    "bail": true,
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "verbose": true
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@storybook/preset-scss": "^1.0.3",
    "@tippyjs/react": "^4.2.6",
    "autoprefixer": "^10.4.14",
    "react-scripts": "^5.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "viem": "^1.2.9",
    "wagmi": "^1.3.4"
  }
